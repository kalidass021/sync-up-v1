{
  "name": "api",
  "version": "1.0.0",
  "description": "api for sync-up, a social media application",
  "main": "./src/bin/www.js",
  "scripts": {
    "build": "babel src -d dist",
    "dev": "nodemon --env-file=.env --require @babel/register ./src/bin/www.js",
    "start": "nodemon dist/bin/www.js",
    "ui": "cd ../ui && npm run dev",
    "api": "npm run dev",
    "app": "concurrently \"npm run api\" \"npm run ui\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Kalidass Boopathi",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.9",
    "@babel/preset-env": "^7.26.9",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.3",
    "cloudinary": "^2.5.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "express": "^4.21.1",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.8.3",
    "nodemon": "^3.1.7"
  },
  "devDependencies": {
    "@babel/register": "^7.25.9",
    "concurrently": "^9.1.2"
  }
}
